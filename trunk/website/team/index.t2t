%!preproc: '(flag/fi.gif])'	\1 Finland
%!preproc: '(flag/fr.gif])'	\1 France
%!preproc: '(flag/hu.gif])'	\1 Hungary
%!preproc: '(flag/it.gif])'	\1 Italy
%!preproc: '(flag/ja.gif])'	\1 Japan
%!preproc: '(flag/lu.gif])'	\1 Luxembourg
