%!preproc: '(flag/uk.gif])'	\1 Ukraine
%!preproc: '(flag/us.gif])'	\1 United States
%!preproc: '(flag/zh.gif])'	\1 China

% Add class to flags
%!postproc: '(flag/...gif")'	\1 class="flag"

% Close table cell when closing lists
%!postproc: (</UL>)		\1</td></tr>

