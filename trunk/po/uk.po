msgid "%s version %s <%s>"
msgstr "%s версія %s <%s>"

msgid "Usage: %s [OPTIONS] [infile.t2t ...]"
msgstr "Використання: %s [ОПЦІЇ] [вхідний_файл.t2t ...]"

msgid "  -t, --target=TYPE   set target document type. currently supported:"
msgstr "  -t, --target=TYPE   встановлює тип вихідного документа. Підтримуються:"

msgid "  -i, --infile=FILE   set FILE as the input file name ('-' for STDIN)"
msgstr "  -i, --infile=FILE   використати FILE як ім'я вхідного файлу ('-' для STDIN)"

msgid "  -o, --outfile=FILE  set FILE as the output file name ('-' for STDOUT)"
msgstr "  -o, --outfile=FILE  використати FILE як ім'я вихідного файлу ('-' для STDOUT)"

msgid "  -H, --no-headers    suppress header, title and footer contents"
msgstr "  -H, --no-headers    не вставляти заголовки та закінчення документа (генерувати тільки тіло)"

msgid "      --headers       show header, title and footer contents (default ON)"
msgstr "      --headers       генерувати заголовки та футер (ввімкнено за замовчуванням)"

msgid "      --encoding=ENC  set target file encoding (utf-8, iso-8859-1, etc)"
msgstr "      --encoding=ENC  зазначити кодування файлу (utf-8, iso-8859-1, і т.п)"

msgid "      --style=FILE    use FILE as the document style (like HTML CSS)"
msgstr "      --style=FILE    використати FILE в якості стилю документа (Як CSS HTML)"

msgid "      --css-sugar     insert CSS-friendly tags for HTML and XHTML targets"
msgstr "      --css-sugar     вставити теги, що краще підходять для CSS (в HTML та XHTML документах)"

msgid "      --css-inside    insert CSS file contents inside HTML/XHTML headers"
msgstr "      --css-inside    ставити зміст CSS файлу в заголовки документа HTML/XHTML"

msgid "      --mask-email    hide email from spam robots. x@y.z turns <x (a) y z>"
msgstr "      --mask-email    cховати адреси email від спам-роботів. x@y.z стане <x (a) y z"

msgid "      --toc           add TOC (Table of Contents) to target document"
msgstr "      --toc           додати зміст до вихідного документа"

msgid "      --toc-only      print document TOC and exit"
msgstr "      --toc-only      тільки надрукувати зміст"

msgid "      --toc-level=N   set maximum TOC level (depth) to N"
msgstr "      --toc-level=N   встановити максимальну глибину змісту (кількість вкладених підрозділів що потрапляють в зміст) в N"

msgid "  -n, --enum-title    enumerate all titles as 1, 1.1, 1.1.1, etc"
msgstr "  -n, --enum-title    пронумерувати всі заголоки як 1, 1.1, 1.1.1, і т.д."

msgid "  -C, --config-file=F read config from file F"
msgstr "  -С, --config-file=F читати конфігурацію з файла F"

msgid "      --rc            read user config file ~/.txt2tagsrc (default ON)"
msgstr "      --rc            читати файл конфігурації користувача ~/txt2tagsrc (за замовчуванням так)"

msgid "      --gui           invoke Graphical Tk Interface"
msgstr "      --gui           запустити графічний інтерфейс"

msgid "  -q, --quiet         quiet mode, suppress all output (except errors)"
msgstr "  -q, --quiet         тихий режим, ввесь вивід припиняється (крім помилок)"

msgid "  -v, --verbose       print informative messages during conversion"
msgstr "  -v, --verbose       друкувати інформативні повідомлення протягом трансляції"

msgid "  -h, --help          print this help information and exit"
msgstr "  -h, --help          надрукувати цю довідку та вийти"

msgid "  -V, --version       print program version and exit"
msgstr "  -V, --version       надрукувати версію програми та вийти"

msgid "      --dump-config   print all the config found and exit"
msgstr "      --dump-config   надрукувати всі знайдені опції та вийти"

msgid "      --dump-source   print the document source, with includes expanded"
msgstr "      --dump-source   надрукувати код, з розкритими включеннями"

msgid "Turn OFF options:"
msgstr "Опції ВИМКНЕННЯ:"

msgid "Example:\n     %s -t html --toc myfile.t2t"
msgstr "Приклад:\n     %s -t html --toc myfile.t2t"

msgid "By default, converted output is saved to 'infile.<target>'."
msgstr "За замовчуванням, згенерований документ зберігається в 'infile.<target>'."

msgid "Use --outfile to force an output file name."
msgstr "Використайте --outfile щоб задати ім'я вихідного файлу."

msgid "If  input file is '-', reads from STDIN."
msgstr "Якщо вхідний файл '-', читає зі стандартного потоку вводу."

msgid "If output file is '-', dumps output to STDOUT."
msgstr "Якщо вихідний файл '-', виводить документ в стандартний потік виводу."

