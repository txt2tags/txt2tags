msgid "%s version %s <%s>"
msgstr "%s %s bertsioa <%s>"

msgid "Usage: %s [OPTIONS] [infile.t2t ...]"
msgstr "Erabilpena: %s [AUKERAK] [sarfitx.t2t ...]"

msgid ""
"      --targets       print a list of all the available targets and exit"
msgstr ""
"      --targets       eskuragarri dauden helburu guztiak erakutsi eta irten"

msgid "  -t, --target=TYPE   set target document type. currently supported:"
msgstr ""
"  -t, --target=MOTA   helburuko dokumentuaren mota ezarri. Momentuz "
"jasandakoak:"

msgid "  -i, --infile=FILE   set FILE as the input file name ('-' for STDIN)"
msgstr ""
"  -i, --infile=FITX   FITX zehaztu sarrera fitxategi bezela ('-' STDIN-"
"entzako)"

msgid "  -o, --outfile=FILE  set FILE as the output file name ('-' for STDOUT)"
msgstr ""
"  -o, --outfile=FITX  FITX zehaztu irteera fitxategiaren izen bezala ('-' "
"STDOUT-entzako)"

msgid "      --encoding=ENC  set target file encoding (utf-8, iso-8859-1, etc)"
msgstr ""
"      --encoding=KOD  ezarri irteera fitxategiaren kodeketa (utf-8, iso-8859-"
"1, etab.)"

msgid "      --toc           add an automatic Table of Contents to the output"
msgstr "      --toc           helburu dokumentura Eduki Taula automatikoa gehitu"

msgid "      --toc-level=N   set maximum TOC level (depth) to N"
msgstr "      --toc-level=N   TOC-aren maila (sakonera) maximoa N-n ezarri"

msgid "      --toc-only      print the Table of Contents and exit"
msgstr "      --toc-only      dokumentuaren Eduki Taula inprimatu eta irten"

msgid "  -n, --enum-title    enumerate all titles as 1, 1.1, 1.1.1, etc"
msgstr "  -n, --enum-title    izenburu guztiak zenbakitu: 1, 1.1, 1.1.1, etab."

msgid "      --style=FILE    use FILE as the document style (like HTML CSS)"
msgstr ""
"      --style=FITX    FITX erabili dokumentuaren estilo moduan (HTML-ko CSS-"
"ak bezala)"

msgid "      --css-sugar     insert CSS-friendly tags for HTML/XHTML"
msgstr "      --css-sugar     CSS-rekin jatorrak diren etiketak txertatu HTML/XHTML-rentzako"

msgid ""
"      --css-inside    insert CSS file contents inside HTML/XHTML headers"
msgstr ""
"      --css-inside    CSS fitxategiaren edukiak HTML/XHTML goiburuetan "
"txertatu"

msgid "  -H, --no-headers    suppress header and footer from the output"
msgstr "  -H, --no-headers    helburuko dokumentuan goiburukoa eta oinaren edukiak kendu"

msgid ""
"      --mask-email    hide email from spam robots. x@y.z turns <x (a) y z>"
msgstr ""
"      --mask-email    epostak spam robotengandik ezkutatu. x@y.z <x (a) y z> "
"bihurtzen da"

msgid ""
"      --slides        format output as presentation slides (used by -t art)"
msgstr ""
"      --slides        irteera aurkezpen diapositiba bezala formateatu (-t "
"art-ekin erabilita)"

msgid ""
"      --width=N       set the output's width to N columns (used by -t art)"
msgstr ""
"      --width=N       irteeraren zabalera N zutabeetan finkatu (-t art-ekin "
"erabilita)"

msgid ""
"      --height=N      set the output's height to N rows (used by -t art)"
msgstr ""
"      --height=N       irteeraren altuera N zutabeetan finkatu (-t art-ekin "
"erabilita)"

msgid "  -C, --config-file=F read configuration from file F"
msgstr "  -C, --config-file=F konfigurazio F fitxategitik irakurri"

msgid "      --gui           invoke Graphical Tk Interface"
msgstr "      --gui           Tk Interfaze Grafikoa abiatu"

msgid "  -q, --quiet         quiet mode, suppress all output (except errors)"
msgstr "  -q, --quiet         modu geldia, irteera mezurik ez (erroreak ezik)"

msgid "  -v, --verbose       print informative messages during conversion"
msgstr ""
"  -v, --verbose       bihurketaren bitartean mezu informatiboak inprimatu"

msgid "  -h, --help          print this help information and exit"
msgstr "  -h, --help          laguntza mezu hau inprimatu eta irten"

msgid "  -V, --version       print program version and exit"
msgstr "  -V, --version       programaren bertsioa inprimatu eta irten"

msgid "      --dump-config   print all the configuration found and exit"
msgstr ""
"      --dump-config   topatutako konfigurazio guztia inprimatu eta irten"

msgid "      --dump-source   print the document source, with includes expanded"
msgstr ""
"      --dump-source   dokumentuaren iturburua inprimatu, barneratzeko "
"guztiak txertatuta"

msgid "Turn OFF options:"
msgstr "Aukerak EZGAITU:"

msgid "Example:"
msgstr "Adibidea:"

msgid "file.t2t"
msgstr "fitx.t2t"

msgid "By default, converted output is saved to 'infile.<target>'."
msgstr "Lehenetsiz, bihurtutako irteera 'infile.<helburua>'-n gordetzen da."

msgid "Use --outfile to force an output file name."
msgstr "--outfile erabili irteera fitxategi izen bat derrigortzeko."

msgid "If  input file is '-', reads from STDIN."
msgstr "Sarrera fitxategia '-' bada, STDIN-etik irakurri."

msgid "If output file is '-', dumps output to STDOUT."
msgstr "Irteera fitxategia '-' bada, STDOUT-en idatzi."

