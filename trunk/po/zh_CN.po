msgid "%s version %s <%s>"
msgstr "%s 版本 %s <%s>"

msgid "Usage: %s [OPTIONS] [infile.t2t ...]"
msgstr "用法: %s [OPTIONS] [infile.t2t ...]"

msgid "      --targets       print a list of all the available targets and exit"
msgstr "      --targets       输出可能的目标格式,然后退出程序"

msgid "  -t, --target=TYPE   set target document type. currently supported:"
msgstr "  -t, --target=TYPE   设置目标文件类型. 目前支持:"

msgid "  -i, --infile=FILE   set FILE as the input file name ('-' for STDIN)"
msgstr "  -i, --infile=FILE   设置 FILE 作为输入文件名 (标准输入用 '-' 表示)"

msgid "  -o, --outfile=FILE  set FILE as the output file name ('-' for STDOUT)"
msgstr "  -o, --outfile=FILE  设置FILE为输出文件名 (标准输出STDOUT用 '-' 表示)"

msgid "      --encoding=ENC  set target file encoding (utf-8, iso-8859-1, etc)"
msgstr "      --encoding=ENC  设置目标文件编码 (utf-8, iso-8859-1, 等)"

msgid "      --toc           add an automatic Table of Contents to the output"
msgstr "      --toc           自动在输出文件添加一个目录"

msgid "      --toc-level=N   set maximum TOC level (depth) to N"
msgstr "      --toc-level=N   设置目录(TOC)最大深度为N"

msgid "      --toc-only      print the Table of Contents and exit"
msgstr "      --toc-only      输出目录,然后退出程序"

msgid "  -n, --enum-title    enumerate all titles as 1, 1.1, 1.1.1, etc"
msgstr "  -n, --enum-title    标题行格式为 1, 1.1, 1.1.1, 等"

msgid "      --style=FILE    use FILE as the document style (like HTML CSS)"
msgstr "      --style=FILE    设置文档样式类型(比如 HTML CSS)"

msgid "      --css-sugar     insert CSS-friendly tags for HTML/XHTML"
msgstr "      --css-sugar     在HTML/XHTML中插入友好的CSS标记"

msgid "      --css-inside    insert CSS file contents inside HTML/XHTML headers"
msgstr "      --css-inside    插入 CSS 文件内容到 HTML/XHTML 文件头部"

msgid "  -H, --no-headers    suppress header and footer from the output"
msgstr "  -H, --no-headers    抑制页眉和页脚从输出"

msgid "      --mask-email    hide email from spam robots. x@y.z turns <x (a) y z>"
msgstr "      --mask-email    防垃圾邮件,隐藏Email地址. x@y.z turns <x (a) y z>"

msgid "      --slides        format output as presentation slides (used by -t art)"
msgstr "      --slides        输出格式作为简介幻灯片 (-t art时使用)"

msgid "      --width=N       set the output's width to N columns (used by -t art)"
msgstr "      --width=N       设置输出宽度为N列(-t art时使用)"

msgid "      --height=N      set the output's height to N rows (used by -t art)"
msgstr "      --height=N      设置输出高度为N行 (-t art时使用)"

msgid "  -C, --config-file=F read configuration from file F"
msgstr "  -C, --config-file=F 读取配置文件为F的配置"

msgid "      --gui           invoke Graphical Tk Interface"
msgstr "      --gui           使用图形Tk界面"

msgid "  -q, --quiet         quiet mode, suppress all output (except errors)"
msgstr "  -q, --quiet         安静模式, 禁用所有输出(错误信息仍输出)"

msgid "  -v, --verbose       print informative messages during conversion"
msgstr "  -v, --verbose       转换时显示转换情况信息"

msgid "  -h, --help          print this help information and exit"
msgstr "  -h, --help          显示本帮助信息"

msgid "  -V, --version       print program version and exit"
msgstr "  -V, --version       显示程序版本并退出"

msgid "      --dump-config   print all the configuration found and exit"
msgstr "      --dump-config   显示所有能找到的配置文件并退出"

msgid "      --dump-source   print the document source, with includes expanded"
msgstr "      --dump-source   展开显示所有源文件"

msgid "Turn OFF options:"
msgstr "关闭的选项:"

msgid "Example:"
msgstr "例如:"

msgid "file.t2t"
msgstr "file.t2t"

msgid "By default, converted output is saved to 'infile.<target>'."
msgstr "默认输出文件保存到'infile.<target>'."

msgid "Use --outfile to force an output file name."
msgstr "使用 --outfile 定制输出文件名."

msgid "If  input file is '-', reads from STDIN."
msgstr "如果输入文件是 '-', 则从标准输入设备(STDIN)读取."

msgid "If output file is '-', dumps output to STDOUT."
msgstr "如果输出文件是 '-', 则输出到标准输出设备(STDOUT)."

