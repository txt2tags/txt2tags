msgid "%s version %s <%s>"
msgstr "%s versão %s <%s>"

msgid "Usage: %s [OPTIONS] [infile.t2t ...]"
msgstr "Uso: %s [OPÇÕES] [arquivo.t2t ...]"

msgid ""
"      --targets       print a list of all the available targets and exit"
msgstr ""
"      --targets       mostra a lista de todos os formatos disponíveis e sai"

msgid "  -t, --target=TYPE   set target document type. currently supported:"
msgstr "  -t, --target=TIPO   define o formato do destino. tipos suportados:"

msgid "  -i, --infile=FILE   set FILE as the input file name ('-' for STDIN)"
msgstr "  -i, --infile=ARQ    define ARQ como o arquivo fonte ('-' para STDIN)"

msgid "  -o, --outfile=FILE  set FILE as the output file name ('-' for STDOUT)"
msgstr ""
"  -o, --outfile=ARQ   define ARQ como o arquivo destino ('-' para STDOUT)"

msgid "      --encoding=ENC  set target file encoding (utf-8, iso-8859-1, etc)"
msgstr ""
"      --encoding=ENC  informa a codificação de destino (utf-8, iso-8859-1, "
"etc)"

msgid "      --toc           add an automatic Table of Contents to the output"
msgstr "      --toc           cria um Sumário automático no documento"

msgid "      --toc-level=N   set maximum TOC level (depth) to N"
msgstr "      --toc-level=N   define o nível máximo do Sumário para N"

msgid "      --toc-only      print the Table of Contents and exit"
msgstr "      --toc-only      exibe o Sumário do documento e sai"

msgid "  -n, --enum-title    enumerate all titles as 1, 1.1, 1.1.1, etc"
msgstr "  -n, --enum-title    numera todos os títulos como 1, 1.1, 1.1.1, etc"

msgid "      --style=FILE    use FILE as the document style (like HTML CSS)"
msgstr ""
"      --style=ARQ     utiliza ARQ como estilo do documento (CSS no Html)"

msgid "      --css-sugar     insert CSS-friendly tags for HTML/XHTML"
msgstr "      --css-sugar     usa tags compatíveis com CSS no HTML/XHTML"

msgid ""
"      --css-inside    insert CSS file contents inside HTML/XHTML headers"
msgstr ""
"      --css-inside    insere o conteúdo do arquivo CSS no documento HTML/"
"XHTML"

msgid "  -H, --no-headers    suppress header and footer from the output"
msgstr "  -H, --no-headers    remove os cabeçalhos e o rodapé do documento"

msgid ""
"      --mask-email    hide email from spam robots. x@y.z turns <x (a) y z>"
msgstr ""
"      --mask-email    esconde e-mails dos robôs de SPAM. x@y.z vira <x (a) y "
"z>"

msgid ""
"      --slides        format output as presentation slides (used by -t art)"
msgstr ""
"      --slides        cria uma apresentação de slides (usado por -t art)"

msgid ""
"      --width=N       set the output's width to N columns (used by -t art)"
msgstr "      --width=N       define a largura em N colunas (usado por -t art)"

msgid ""
"      --height=N      set the output's height to N rows (used by -t art)"
msgstr "      --height=N      define a altura em N linhas (usado por -t art)"

msgid "  -C, --config-file=F read configuration from file F"
msgstr "  -C, --config-file=F carrega arquivo de configuração F"

msgid "      --gui           invoke Graphical Tk Interface"
msgstr "      --gui           carrega a interface gráfica (Tk)"

msgid "  -q, --quiet         quiet mode, suppress all output (except errors)"
msgstr "  -q, --quiet         modo silencioso, sem mensagens (exceto erros)"

msgid "  -v, --verbose       print informative messages during conversion"
msgstr "  -v, --verbose       exibe mensagens informativas durante a conversão"

msgid "  -h, --help          print this help information and exit"
msgstr "  -h, --help          exibe este texto de ajuda e sai"

msgid "  -V, --version       print program version and exit"
msgstr "  -V, --version       exibe a versão do programa e sai"

msgid "      --dump-config   print all the configuration found and exit"
msgstr "      --dump-config   exibe todas as configurações encontradas e sai"

msgid "      --dump-source   print the document source, with includes expanded"
msgstr "      --dump-source   mostra os fontes com os includes expandidos"

